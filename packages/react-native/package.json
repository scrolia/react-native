{
    "name": "@scrolia/react-native",
    "version": "0.6.2",
    "description": "A headless scrollbar component",
    "keywords": [
        "scrolia",
        "scrollbar",
        "react",
        "react-native",
        "react-native-web",
        "headless",
        "component",
        "ts",
        "typescript",
        "js",
        "javascript"
    ],
    "homepage": "https://github.com/scrolia/react-native",
    "bugs": "https://github.com/scrolia/react-native/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/scrolia/react-native.git",
        "directory": "packages/react-native"
    },
    "license": "MIT",
    "author": {
        "name": "Alpheus",
        "email": "contact@alphe.us"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./contexts/scrollcore": {
            "types": "./dist/contexts/scrollcore.d.ts",
            "import": "./dist/contexts/scrollcore.mjs",
            "require": "./dist/contexts/scrollcore.js"
        },
        "./functions/props": {
            "types": "./dist/functions/props.d.ts",
            "import": "./dist/functions/props.mjs",
            "require": "./dist/functions/props.js"
        },
        "./hooks/content": {
            "types": "./dist/hooks/content/index.d.ts",
            "import": "./dist/hooks/content/index.mjs",
            "require": "./dist/hooks/content/index.js"
        },
        "./hooks/thumb": {
            "types": "./dist/hooks/thumb/index.d.ts",
            "import": "./dist/hooks/thumb/index.mjs",
            "require": "./dist/hooks/thumb/index.js"
        },
        "./package.json": "./package.json"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "dependencies": {
        "ts-vista": "catalog:dep"
    },
    "devDependencies": {
        "@types/react": "catalog:dev",
        "react": "catalog:dev",
        "react-native": "catalog:dev"
